{
  "name": "revelled",
  "description": "A public event management and advertisement platform for interactive events.",
  "version": "1.0.0",
  "author": {
    "name": "Martin Velkov",
    "url": "https://github.com/MartsTech"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartsTech/Revelled.git"
  },
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "npm run build && next export -o _static",
    "postbuild": "move _static ../api/src/API/wwwroot",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "format": "prettier --write ./src",
    "check-format": "prettier --check ./src",
    "check-types": "tsc --pretty --noEmit",
    "test-all": "yarn check-format && yarn lint && yarn check-types && yarn test && yarn build",
    "gen-env": "npx gen-env-types .env.local -o src/types/env.d.ts -e ."
  },
  "dependencies": {
    "axios": "^0.25.0",
    "date-fns": "^2.28.0",
    "mobx": "^6.3.13",
    "mobx-react-lite": "^3.2.3",
    "next": "^12.0.8",
    "next-pwa": "5.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/styled-components": "^5.1.21",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-testing-library": "^5.0.4",
    "gen-env-types": "^1.3.2",
    "jest": "^27.4.7",
    "jest-styled-components": "^7.0.8",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "stylelint": "^14.2.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.5.5"
  }
}
